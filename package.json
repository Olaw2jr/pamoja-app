{
  "name": "sushi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:storybook": "ENVFILE=.env.storybook npm run android",
    "ios": "react-native run-ios --simulator=\"iPhone 14\"",
    "start": "react-native start",
    "test": "jest ./*.unit.test.*",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate": "plop",
    "android:debug": "npm run android:bundle-js && npm run android:build-debug-apk",
    "android:release": "cd android && ./gradlew bundleRelease && cd ..",
    "android:bundle-js": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "android:build-debug-apk": "cd android && ./gradlew assembleDebug && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.5",
    "@storybook/react-native": "^7.6.11",
    "@types/react": "^17.0.80",
    "date-fns": "^3.6.0",
    "numbro": "^2.5.0",
    "ramda": "^0.30.1",
    "react": "18.3.1",
    "react-native": "0.77.0",
    "react-native-calendars": "^1.1305.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-config": "^1.5.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.32.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.3.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "rn-fetch-blob": "^0.12.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.12",
    "@types/ramda": "^0.30.0",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "plop": "^4.0.1",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.5.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
